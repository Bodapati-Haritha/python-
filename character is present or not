str1 =str(input("String="))
c =str(input("Enter character to be find:"))
res = None
for i in range(0, len(str1)):
    if str1[i] == c:
        res = i + 1
        break    
if res == None:
    print ("No such character available in string")
else:
    print ("Character {} is present at {}".format(str,(res)))
